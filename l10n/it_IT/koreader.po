msgid "Z-library"
msgstr "Z-library"

msgid "[[A plugin to search and download books from Z-library.]]"
msgstr "[[Un plugin per cercare e scaricare libri da Z-library.]]"

msgid "Z-library search"
msgstr "Ricerca Z-library"

msgid "Z-library most popular"
msgstr "Z-library più popolari"

msgid "Z-library recommended"
msgstr "Z-library consigliati"

msgid "Settings"
msgstr "Impostazioni"

msgid "Set base URL"
msgstr "Imposta URL base"

msgid "Invalid Base URL."
msgstr "URL base non valido."

msgid "Set email"
msgstr "Imposta email"

msgid "Set password"
msgstr "Imposta password"

msgid "Verify credentials"
msgstr "Verifica credenziali"

msgid "Login successful!"
msgstr "Accesso riuscito!"

msgid "Set download directory"
msgstr "Imposta directory di download"

msgid "Select search languages"
msgstr "Seleziona lingue di ricerca"

msgid "Select search formats"
msgstr "Seleziona formati di ricerca"

msgid "Check for updates"
msgstr "Controlla aggiornamenti"

