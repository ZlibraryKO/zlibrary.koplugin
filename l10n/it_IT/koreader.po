msgstr "Impossibile recuperare i dettagli del libro"

msgid "Could not retrieve book details."
msgstr "Impossibile recuperare i dettagli del libro."

msgid "Please set both username and password first."
msgstr "Per favore, imposta prima nome utente e password."

msgid "Logging in..."
msgstr "Accesso in corso..."

msgid "Search failed due to a temporary issue (HTTP 400). Would you like to retry?"
msgstr "Ricerca fallita a causa di un problema temporaneo (HTTP 400). Vuoi riprovare?"

msgid "Retry"
msgstr "Riprova"

msgid "Cancel"
msgstr "Annulla"

msgid "Retrying search for \""
msgstr "Riprovo la ricerca per \""

msgid "Searching for \""
msgstr "Ricerca di \""

msgid "Search failed"
msgstr "Ricerca fallita"

msgid "No results found for \""
msgstr "Nessun risultato trovato per \""

#, lua-format
msgid "Loading more results (Page %s)..."
msgstr "Caricamento altri risultati (Pagina %s)..."

msgid "Failed to load more results"
msgstr "Impossibile caricare altri risultati"

msgid "No more results found."
msgstr "Nessun altro risultato trovato."

msgid "No download link available for this book."
msgstr "Nessun link di download disponibile per questo libro."

#, lua-format
