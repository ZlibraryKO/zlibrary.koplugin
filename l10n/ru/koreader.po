msgstr "Выбрать папку для загрузок из Z-библиотеки"

#, lua-format
msgid "Download directory set to: %s"
msgstr "Папка для загрузок установлена: %s"

msgid "No directory selected."
msgstr "Папка не выбрана."

#, lua-format
msgid "%d items selected for %s."
msgstr "Выбрано %d элементов для %s."

#, lua-format
msgid "Filter cleared for %s."
msgstr "Фильтр очищен для %s."

msgid "Setting saved successfully!"
msgstr "Настройки успешно сохранены!"

msgid "Setting cleared."
msgstr "Настройка очищена."

msgid "Search Z-library"
msgstr "Поиск в Z-библиотеке"

msgid "Please enter a search term."
msgstr "Пожалуйста, введите поисковый запрос."

msgid "Unknown Title"
msgstr "Неизвестное название"

msgid "Full Title"
msgstr "Полное название"

msgid "Author: "
msgstr "Автор: "

msgid "Full Author"
msgstr "Полное имя автора"

msgid "Year: "
msgstr "Год: "

#, lua-format
msgid "Format: %s (tap to download)"
msgstr "Формат: %s (нажмите для скачивания)"

#, lua-format
msgid "Format: %s (Download not available)"
msgstr "Формат: %s (Скачивание недоступно)"

msgid "Download Book (Unknown Format)"
msgstr "Скачать книгу (неизвестный формат)"

msgid "Publisher: "
msgstr "Издательство: "

